{
  "version": 2,
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "framework": "vite",
  "public": false,
  "regions": ["sfo1"],
  "routes": [
    {
      "src": "^/api/.*",
      "dest": "server/index.ts"
    },
    {
      "src": "^/.*",
      "dest": "dist/public/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
