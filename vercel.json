{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "vite",
  "public": false,
  "regions": ["sfo1"],
  "functions": {
    "server/index.ts": {
      "runtime": "nodejs@20",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "^/api/.*",
      "dest": "server/index.ts"
    },
    {
      "src": "^/.*",
      "dest": "dist/public/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
